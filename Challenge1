pragma solidity ^0.8.4;

contract readAndWrite {

    uint num;

    function get() public view returns (uint) {

        // reading the value of the variable

        return num;
    }
    

    function set(uint _num) public {

        //Writing a value of the variable

        num = _num;
    }
}
